<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.video_chat">

    <uses-permission android:name="android.permission.INTERNETR" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/epidoum"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CalendarActivity"></activity>
        <activity android:name=".FAQ" />
        <activity android:name=".ChatMainActivity" />
        <activity android:name=".Follow_us" />
        <activity android:name=".MedicineReminder" />
        <activity android:name=".ContactActivity" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".ProfileActivityChat" />
        <activity android:name=".MyList" />
        <activity android:name=".prescription" />
        <activity android:name=".videoChatActivity" />
        <activity android:name=".callingActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".FindPeopleActivity" />
        <activity android:name=".NotificationsActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".RegistrationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".MyBroadcastReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example..BroadcastReceiver" />
            </intent-filter>
        </receiver>
    </application>

</manifest>